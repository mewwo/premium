						local e = Instance.new("BodyVelocity", child)
						while peniscock and peniscock.Parent ~= nil do
							game.RunService.Stepped:wait()
							e.Velocity = ((child.CFrame * CFrame.new(0, -speed, 0)).p - child.CFrame.p)
						end

					end
				end)

				selectionBox = Enum.Material.Neon
				selectionBox.Color3 = Color3.new(1, 1, 1)  

				LP.Character.LeftHand.Size = penis 

				LP.Character.BodyEffects.SpecialParts.LeftHand.Size = penis 




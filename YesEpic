									bill.Adornee = v.Character.Head
									bill.Visible = false
									bill.ResetOnSpawn = false
								end
								for a,c in pairs(v.Character:GetChildren()) do
									if c.ClassName == "MeshPart" and c.Transparency ~= 1 then
										doit(c)
									elseif c.ClassName == "Part" and c.Transparency ~= 1 then

						v:Destroy()
					end
				end


				-- Gui to Lua
				-- Version: 3.2

				-- Instances:

				local ScreenGui = Instance.new("ScreenGui")
				local lmao = Instance.new("Frame")
				local Target = Instance.new("ScrollingFrame")
				local TextBox = Instance.new("TextBox")
				local UIListLayout = Instance.new("UIListLayout")
